{
  "name": "nadswap-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "deps:system": "bash ./scripts/install_all_deps.sh",
    "deps:check": "bash ./scripts/install_all_deps.sh --check-only",
    "deps:all": "pnpm deps:system && pnpm install",
    "setup": "pnpm deps:all",
    "setup:check": "pnpm deps:check",
    "deploy:local": "./scripts/deploy_local.sh",
    "test:gates": "bash ./scripts/runners/run_local_gates.sh --skip-fork",
    "test:lens": "bash ./scripts/runners/run_lens_tests.sh --skip-fork",
    "test:fork": "bash ./scripts/runners/run_fork_tests.sh",
    "test:local": "bash ./scripts/runners/run_local_gates.sh --skip-fork --dev && bash ./scripts/runners/run_lens_tests.sh --skip-fork",
    "test:all": "./scripts/run_all_tests.sh",
    "local:up": "bash ./scripts/frontend/local-up.sh",
    "dev:nadswap": "pnpm --filter @nadswap/nadswap dev",
    "dev:nadswap:local": "pnpm local:up",
    "dev:nadswap:local:quick": "bash ./scripts/frontend/dev-local.sh",
    "build:nadswap": "pnpm --filter @nadswap/nadswap build",
    "lint:nadswap": "pnpm --filter @nadswap/nadswap lint",
    "test:nadswap": "pnpm --filter @nadswap/nadswap test",
    "env:sync:nadswap": "bash ./scripts/frontend/sync-local-env.sh"
  }
}
